{{!-- code creates a dynamic list of fuel stations with their names, addresses, prices per unit, and distances from the user's location. --}}
<h1>Fuel Prices Near You</h1>
<ul id="fuel-prices">
  {{#each fuelStations}}
    <li>
      <h2>{{name}}</h2>
      <p>Address: {{address}}</p>
      <p>Price per Unit: {{price}}</p>
      <p>Distance: {{distance}} km</p>
    </li>
  {{/each}}
</ul>
